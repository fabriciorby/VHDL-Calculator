`timescale 1 ps/ 1 ps
module binary4_to_bcd8_vector_vlg_sample_tst(
	BIN,
	sampler_tx
);
input [3:0] BIN;
output sampler_tx;

reg sample;
time current_time;
always @(BIN)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module binary4_to_bcd8_vector_vlg_check_tst (
	BCD0,
	BCD1,
	sampler_rx
);
input [3:0] BCD0;
input [3:0] BCD1;
input sampler_rx;

reg [3:0] BCD0_expected;
reg [3:0] BCD1_expected;

reg [3:0] BCD0_prev;
reg [3:0] BCD1_prev;

reg [3:0] BCD0_expected_prev;
reg [3:0] BCD1_expected_prev;

reg [3:0] last_BCD0_exp;
reg [3:0] last_BCD1_exp;

reg trigger;

integer i;
integer nummismatches;

reg [1:2] on_first_change ;


initial
begin
trigger = 0;
i = 0;
nummismatches = 0;
on_first_change = 2'b1;
end

// update real /o prevs

always @(trigger)
begin
	BCD0_prev = BCD0;
	BCD1_prev = BCD1;
end

// update expected /o prevs

always @(trigger)
begin
	BCD0_expected_prev = BCD0_expected;
	BCD1_expected_prev = BCD1_expected;
end


// expected BCD0[ 3 ]
initial
begin
	BCD0_expected[3] = 1'bX;
end 
// expected BCD0[ 2 ]
initial
begin
	BCD0_expected[2] = 1'bX;
end 
// expected BCD0[ 1 ]
initial
begin
