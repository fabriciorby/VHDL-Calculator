		nummismatches = nummismatches + 1;
		on_first_change[1] = 1'b0;
		last_BCD0_exp[3] = BCD0_expected_prev[3];
	end
	if (
		( BCD1_expected_prev[0] !== 1'bx ) && ( BCD1_prev[0] !== BCD1_expected_prev[0] )
		&& ((BCD1_expected_prev[0] !== last_BCD1_exp[0]) ||
